{
  "name": "yogram",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "rabbitmq": "docker-compose -f docker-compose.rabbitmq.yaml up -d",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/gate/main",
    "build:gate": "nest build gate",
    "start:gate": "node dist/apps/gate/main",
    "start:dev:gate": "cross-env NODE_ENV=DEVELOPMENT nest start gate --watch",
    "build:users": "nest build users",
    "start:users": "node dist/apps/users/main",
    "start:dev:users": "cross-env NODE_ENV=DEVELOPMENT nest start users --watch",
    "build:mailer": "nest build mailer",
    "start:mailer": "node dist/apps/mailer/main",
    "start:dev:mailer": "cross-env NODE_ENV=DEVELOPMENT nest start mailer --watch",
    "build:posts": "nest build posts",
    "start:posts": "node dist/apps/posts/main",
    "start:dev:posts": "cross-env NODE_ENV=DEVELOPMENT nest start posts --watch",
    "build:files": "nest build files",
    "start:files": "node dist/apps/files/main",
    "start:dev:files": "cross-env NODE_ENV=DEVELOPMENT nest start files --watch",
    "build:business": "nest build business",
    "start:business": "node dist/apps/business/main",
    "start:dev:business": "cross-env NODE_ENV=DEVELOPMENT nest start business --watch",
    "migration": "cross-env node scripts/typeorm-migration.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=TESTING jest --config ./apps/gate/test/jest-e2e.json",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli"
  },
  "dependencies": {
    "@apollo/gateway": "^2.10.2",
    "@apollo/server": "^4.12.0",
    "@apollo/subgraph": "^2.10.2",
    "@aws-sdk/client-s3": "^3.828.0",
    "@aws-sdk/lib-storage": "^3.828.0",
    "@faker-js/faker": "^9.7.0",
    "@golevelup/nestjs-discovery": "^5.0.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.13",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cache-manager": "^7.1.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "form-data": "^4.0.3",
    "graphql": "^16.11.0",
    "ioredis": "^5.6.1",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "pg": "^8.15.6",
    "recaptcha-node": "^0.2.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.2",
    "typeorm": "^0.3.23",
    "uuid": "^11.1.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/apps/libs/",
      "<rootDir>/apps/gate/test"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/yogram(|/.*)$": "<rootDir>/libs/yogram/src/$1"
    }
  }
}
